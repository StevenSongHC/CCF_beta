<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ccf.bean.City" table="city" catalog="beta">
        <id name="id" type="integer">
            <column name="ctid" />
            <generator class="native" />
        </id>
        <property name="name" type="string">
            <column name="ct_name" length="20" not-null="true" />
        </property>
        <property name="cnName" type="string">
            <column name="ct_cn_name" length="20" />
        </property>
        <property name="collegeAmount" type="integer">
            <column name="ct_college_amount" length="3" />
        </property>
        <property name="prid" type="integer">
            <column name="ct_prid" length="3" />
        </property>
    </class>
    
</hibernate-mapping>